Game Objects:
  - Game Object:
      Name: Game Object
      Transform:
        Translation: [0, 0, 0]
        Rotation: [270, 0, 0]
        Scale: [1, 1, 1]
      Components:
        Mesh Renderer:
          Material: Material.0
        Mesh:
          Path: assets/gun.fbx
  - Game Object:
      Name: Camera
      Transform:
        Translation: [0, 0, 10]
        Rotation: [0, 0, 0]
        Scale: [1, 1, 1]
      Components:
        Camera:
          Main: true
          Projection Type: Perspective
          Orthographic:
            Size: 10
            Near Clip: -1
            Far Clip: 1
          Perspective:
            FOV: 45
            Near Clip: 0.100000001
            Far Clip: 100